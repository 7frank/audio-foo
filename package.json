{
  "name": "audio-fu",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --no-cache  --continue --concurrency 12",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "lint:fix": "pnpm lint -- --fix",
    "test": "turbo run test",
    "rimraf": "find . -maxdepth 2 -name node_modules -type d -exec rm -rf {} +"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "turbo": "^1.10.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@pixi/utils"
      ],
      "allowedVersions": {
        "svelte": "^4"
      }
    }
  }
}
